            "details": {
              "to": {
                "type": "field",
                "value": "jump_to_field_reference"
              }
            },
            "condition": {
              "op": "equal",
              "vars": [
                {
                  "type": "field",
                  "value": "jump_if_field reference"
                },
                {
                  "type": "constant",
                  "value": 5
                }
              ]
            }
          },
          {
            "action": "add",
            "details": {
              "target": {
                "type": "variable",
                "value": "score"
              },
              "value": {
                "type": "constant",
                "value": 5
              }
            },
            "condition": {
              "op": "is",
              "vars": [
                {
                  "type": "field",
                  "value": "triggering_field_reference"
                },
                {
                  "type": "constant",
                  "value": true
                }
              ]
